{
    "manifest": {
        "displayName": "Voice car",
        "invocationName": "따라와",
        "category": "PRODUCTIVITY"
    },
    "actions": [
        {
            "name": "com.voiceCar.actions.move",
            "availability": {
                "deviceClasses": [
                    {
                        "assistantSdkDevice": {}
                    }
                ]
            },
            "intent": {
                "name": "com.voiceCar.intents.move",
                "parameters": [
                    {
                        "name": "direction",
                        "type": "SchemaOrg_Text"
                    }
                ],
                "trigger": {
                    "queryPatterns": [
                        "$SchemaOrg_Text:direction 으로가",
                        "$SchemaOrg_Text:direction 로가",
                        "$SchemaOrg_Text:direction 회전",
                        "$SchemaOrg_Text:direction"
                    ]
                }
            },
            "fulfillment": {
                "staticFulfillment": {
                    "templatedResponse": {
                        "items": [
                            {
                                "simpleResponse": {
                                    "textToSpeech": "$direction 로 갈게요"
                                }
                            },
                            {
                                "deviceExecution": {
                                    "command": "com.voiceCar.commands.direction",
                                    "params": {
                                        "direction": "$direction"
                                    }
                                }
                            }
                        ]
                    }
                }
            }
        }
    ],

    "types": [
        {
            "name": "$direction",
            "entities": [
                {
                    "key": "앞",
                    "synonyms": [
                        "전방",
                        "직진",
                        "열두시",
                        "쭉"
                    ]
                },
                {
                    "key": "뒤",
                    "synonyms": [
                        "후진",
                        "빽",
                        "빠져",
                        "여섯시"
                    ]
                },
                {
                    "key": "좌",
                    "synonyms": [
                        "왼쪽",
                        "좌회전",
                        "아홉시"
                    ]
                },
                {
                    "key": "우",
                    "synonyms": [
                        "우측",
                        "세시",
                        "우회전"
                    ]
                }
            ]
        }
    ],
    "locale": "ko"
}